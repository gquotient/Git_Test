<td>
  <input id="org_{{ label }}_name" name="org_{{ label }}_name" type="text" value="{{ name }}">
</td>
<td>
  <select id="org_{{ label }}_type" name="org_{{ label }}_type">
    <option value="unspecified" {{#equal type 'unspecified'}}selected{{/equal}}>Unspecified</option>
    <option value="vendor" {{#equal type 'vendor'}}selected{{/equal}}>Vendor</option>
  </select>
</td>
<td>
  <input id="org_{{ label }}_salesforce_id" name="org_{{ label }}_salesforce_id" type="text" value="{{ salesforce_id }}">
</td>
